{
  "name": "contract-playground",
  "version": "0.1.0",
  "description": "A Hardhat project with TypeScript for playing around with smart contracts.",
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "yarn run build && npx hardhat coverage --temp artifacts --network coverage",
    "postinstall": "yarn run compile",
    "lint": "gts lint",
    "fix": "gts fix",
    "prepare": "cd .. && husky install ./contracts/.husky",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erhant/contract-playground.git"
  },
  "author": "erhant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erhant/contract-playground/issues"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.1.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^4.7.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.1.0",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.14.9",
    "ascii85": "^1.0.2",
    "chai": "^4.3.4",
    "dotenv": "^9.0.2",
    "eciesjs": "^0.3.15",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.4",
    "gts": "^3.1.0",
    "hardhat": "^2.10.0",
    "husky": "^8.0.1",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.2.4"
  }
}
